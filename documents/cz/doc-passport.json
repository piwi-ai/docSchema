{
  "id": "doc-passport",
  "name": "Passport",
  "description": "Passport — Czech Republic",
  "references": [
    {
      "title": "Ministry of Interior — Travel Documents",
      "url": "https://www.mvcr.cz/mvcren/article/travel-documents.aspx",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Jméno"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Příjmení"
      },
      "passportNumber": {
        "type": "string",
        "description": "Passport number"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Státní příslušnost"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Datum narození",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Místo narození"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Pohlaví",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Datum vydání",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Platnost do",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Vydal"
      },
      "issuingCountry": {
        "type": "string",
        "description": "Issuing country / Code"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "passportNumber",
      "nationality",
      "dateOfBirth",
      "sex",
      "issueDate",
      "expirationDate"
    ]
  }
}