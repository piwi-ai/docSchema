{
  "id": "doc-identity-card",
  "name": "Identity Document",
  "description": "Identity document (ID Card, Passport, Driver License)",
  "references": [
    {
      "title": "Politiet — National ID card",
      "url": "https://www.politiet.no/en/services/passports-and-id-cards/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Fornavn"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Etternavn"
      },
      "personalId": {
        "type": "string",
        "description": "Personal ID number / Fødselsnummer",
        "pattern": "^\\d{11}$"
      },
      "documentType": {
        "type": "string",
        "description": "Document type / Dokumenttype"
      },
      "documentNumber": {
        "type": "string",
        "description": "Document number / Dokumentnummer"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Utstedelsesdato",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Utløpsdato",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Utstedende myndighet"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Fødested"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Fødselsdato",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Adresse"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Statsborgerskap"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Kjønn",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "personalId",
      "documentType",
      "documentNumber",
      "issueDate",
      "expirationDate"
    ]
  }
}