          },
          "codiceFiscale": {
            "type": "string",
            "description": "Codice Fiscale di 16 caratteri (extract without spaces - remove all spacing if present)",
            "pattern": "^[A-Z]{6}\\d{2}[A-Z]\\d{2}[A-Z]\\d{3}[A-Z]$"
          },
          "dataNascita": {
            "type": "string",
            "description": "Data di nascita",
            "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
          }
        },
        "required": [
          "nome",
          "cognome",
          "codiceFiscale"
        ]
      },
      "redditoComplessivo": {
        "type": "number",
        "description": "Reddito complessivo in Euro"
