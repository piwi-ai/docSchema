    {
      "id": "doc-1099",
      "name": "1099 Form",
      "description": "IRS Form 1099 (NEC/MISC) - Non-Employee Compensation or Miscellaneous Income",
      "jsonSchema": {
        "type": "object",
        "properties": {
          "formType": {
            "type": "string",
            "description": "Form 1099 type",
            "enum": [
              "NEC",
              "MISC",
              "INT",
              "DIV",
              null
            ],
            "nullable": true
          },
          "year": {
            "type": "number",
            "description": "Calendar year"
          },
          "payerName": {
            "type": "string",
            "description": "Payer's name"
          },
          "payerAddress": {
            "type": "string",
            "description": "Payer's address"
          },
          "payerTIN": {
            "type": "string",
            "description": "Payer's TIN (EIN or SSN)"
          },
          "recipientName": {
            "type": "string",
            "description": "Recipient's name"
          },
          "recipientAddress": {
            "type": "string",
            "description": "Recipient's address"
          },
          "recipientTIN": {
            "type": "string",
            "description": "Recipient's TIN (SSN or EIN)"
          },
          "accountNumber": {
            "type": "string",
            "description": "Account number (optional)"
          },
          "nonEmployeeCompensation": {
            "type": "number",
            "description": "Non-employee compensation (Box 1 of 1099-NEC) (USD, numbers only, no dollar sign)"
          },
          "federalTaxWithheld": {
            "type": "number",
            "description": "Federal income tax withheld (Box 4) (USD, numbers only, no dollar sign)"
          },
          "stateTaxWithheld": {
            "type": "number",
            "description": "State tax withheld (Box 5) (USD, numbers only, no dollar sign)"
          },
          "stateIncome": {
            "type": "number",
            "description": "State income (Box 7) (USD, numbers only, no dollar sign)"
          },
          "state": {
            "type": "string",
            "description": "State abbreviation (Box 6)"
          },
          "payerStateNo": {
            "type": "string",
            "description": "Payer's state no (Box 6)"
          }
        },
        "required": [
          "formType",
          "year",
          "payerName",
          "recipientName"
        ]
      }
    },
    {
      "id": "doc-bank-statement",
      "name": "Bank Statement",
      "description": "Monthly statement of account activity",
      "jsonSchema": {
        "type": "object",
        "properties": {
          "bankName": {
            "type": "string",
            "description": "Name of the bank/financial institution"
          },
          "accountNumber": {
            "type": "string",
            "description": "Account number (masked or full)"
          },
          "accountHolderName": {
            "type": "string",
            "description": "Name of the account holder"
          },
          "statementDate": {
            "type": "string",
            "description": "Statement generation date",
            "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
          },
          "periodStart": {
            "type": "string",
            "description": "Statement period start date",
            "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
          },
          "periodEnd": {
            "type": "string",
            "description": "Statement period end date",
            "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
          },
          "openingBalance": {
            "type": "number",
            "description": "Opening balance (USD, numbers only, no dollar sign)"
          },
          "closingBalance": {
            "type": "number",
            "description": "Closing balance (USD, numbers only, no dollar sign)"
          },
          "totalDeposits": {
            "type": "number",
            "description": "Total amount of deposits/credits (USD, numbers only, no dollar sign)"
          },
          "totalWithdrawals": {
            "type": "number",
            "description": "Total amount of withdrawals/debits (USD, numbers only, no dollar sign)"
          },
          "transactions": {
            "type": "array",
            "description": "List of transactions",
            "items": {
              "type": "object",
              "properties": {
                "date": {
                  "type": "string",
                  "description": "Transaction date",
                  "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
                },
                "description": {
                  "type": "string",
                  "description": "Transaction description"
                },
                "amount": {
                  "type": "number",
                  "description": "Transaction amount (USD, numbers only, no dollar sign)"
                },
                "type": {
                  "type": "string",
                  "description": "Transaction type",
                  "enum": [
                    "credit",
                    "debit",
                    null
                  ],
                  "nullable": true
                },
                "balance": {
                  "type": "number",
                  "description": "Balance after transaction (USD, numbers only, no dollar sign)"
                }
              },
              "required": [
                "date",
                "description",
                "amount"
              ]
            }
          }
        },
        "required": [
          "bankName",
          "periodEnd",
          "closingBalance"
        ]
      }
    },
