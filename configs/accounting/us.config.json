          "invoiceDate": {
            "type": "string",
            "description": "Invoice issue date",
            "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
          },
          "dueDate": {
            "type": "string",
            "description": "Payment due date",
            "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
          },
          "vendorName": {
            "type": "string",
