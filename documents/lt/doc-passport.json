{
  "id": "doc-passport",
  "name": "Passport",
  "description": "Passport — Lithuania",
  "references": [
    {
      "title": "ADIC — Passport",
      "url": "https://adic.lrv.lt/en/identity-documents/passport",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Vardas"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Pavardė"
      },
      "passportNumber": {
        "type": "string",
        "description": "Passport number"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Pilietybė"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Gimimo data",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Gimimo vieta"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Lytis",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Išdavimo data",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Galioja iki",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Išdavusi institucija"
      },
      "issuingCountry": {
        "type": "string",
        "description": "Issuing country / Code"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "passportNumber",
      "nationality",
      "dateOfBirth",
      "sex",
      "issueDate",
      "expirationDate"
    ]
  }
}