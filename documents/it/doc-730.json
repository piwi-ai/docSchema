            },
            "relazione": {
              "type": "string",
              "description": "Relazione",
              "enum": [
                "coniuge",
                "figlio",
                "altro familiare",
                null
              ],
              "nullable": true
            },
            "percentualeCarico": {
              "type": "number",
              "description": "Percentuale a carico (es. 100, 50)"
