{
  "id": "doc-passport",
  "name": "Passport",
  "description": "Passport — France",
  "references": [
    {
      "title": "Service-Public.fr — Passeport",
      "url": "https://www.service-public.fr/particuliers/vosdroits/N360",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Prénom"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Nom"
      },
      "passportNumber": {
        "type": "string",
        "description": "Passport number"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Nationalité"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Date de naissance",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Lieu de naissance"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Sexe",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Date de délivrance",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Date d'expiration",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Autorité de délivrance"
      },
      "issuingCountry": {
        "type": "string",
        "description": "Issuing country / Code"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "passportNumber",
      "nationality",
      "dateOfBirth",
      "sex",
      "issueDate",
      "expirationDate"
    ]
  }
}