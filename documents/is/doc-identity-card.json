{
  "id": "doc-identity-card",
  "name": "Identity Document",
  "description": "Identity document (ID Card, Passport, Driver License)",
  "references": [
    {
      "title": "Registers Iceland — ID",
      "url": "https://www.skra.is/english/individuals/passport-and-id-card/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Fornafn"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Eftirnafn"
      },
      "personalId": {
        "type": "string",
        "description": "Personal ID number / Kennitala",
        "pattern": "^\\d{6}-?\\d{4}$"
      },
      "documentType": {
        "type": "string",
        "description": "Document type / Tegund skjals"
      },
      "documentNumber": {
        "type": "string",
        "description": "Document number / Númer skjals"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Útgáfudagur",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Gildir til",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Útgefandi"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Fæðingarstaður"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Fæðingardagur",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Heimilisfang"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Ríkisfang"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Kyn",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "personalId",
      "documentType",
      "documentNumber",
      "issueDate",
      "expirationDate"
    ]
  }
}