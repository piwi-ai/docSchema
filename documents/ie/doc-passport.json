{
  "id": "doc-passport",
  "name": "Passport",
  "description": "Passport — Ireland",
  "references": [
    {
      "title": "DFA — Passports",
      "url": "https://www.dfa.ie/passports/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Céadainm"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Sloinne"
      },
      "passportNumber": {
        "type": "string",
        "description": "Passport number"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Náisiúntacht"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Dáta breithe",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Áit bhreithe"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Gnéas",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Dáta eisiúna",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Dáta éaga",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Údarás eisiúna"
      },
      "issuingCountry": {
        "type": "string",
        "description": "Issuing country / Code"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "passportNumber",
      "nationality",
      "dateOfBirth",
      "sex",
      "issueDate",
      "expirationDate"
    ]
  }
}