{
  "id": "doc-passport",
  "name": "Passport",
  "description": "Passport — Netherlands",
  "references": [
    {
      "title": "Rijksoverheid — Paspoort",
      "url": "https://www.rijksoverheid.nl/onderwerpen/paspoort-en-identiteitskaart",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Voornamen"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Achternaam"
      },
      "passportNumber": {
        "type": "string",
        "description": "Passport number"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Nationaliteit"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Geboortedatum",
        "pattern": "^\\d{2}-\\d{2}-\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Geboorteplaats"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Geslacht",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Datum van afgifte",
        "pattern": "^\\d{2}-\\d{2}-\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Geldig tot",
        "pattern": "^\\d{2}-\\d{2}-\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Autoriteit"
      },
      "issuingCountry": {
        "type": "string",
        "description": "Issuing country / Code"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "passportNumber",
      "nationality",
      "dateOfBirth",
      "sex",
      "issueDate",
      "expirationDate"
    ]
  }
}