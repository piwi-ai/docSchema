          "taxAmount": {
            "type": "number",
            "description": "Total tax amount (USD, numbers only, no dollar sign)"
          },
          "totalAmount": {
            "type": "number",
            "description": "Total amount due (USD, numbers only, no dollar sign)"
          },
          "paymentStatus": {
            "type": "string",
            "description": "Payment status",
            "enum": [
              "paid",
              "unpaid",
              "overdue",
              "partially_paid",
              null
            ],
            "nullable": true
          },
          "items": {
            "type": "array",
            "description": "Line items listed on the invoice",
