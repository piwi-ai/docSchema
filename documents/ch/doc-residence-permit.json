{
  "id": "doc-residence-permit",
  "name": "Residence Permit",
  "description": "Residence Permit — Switzerland",
  "references": [
    {
      "title": "SEM — Residence",
      "url": "https://www.sem.admin.ch/sem/en/home.html",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Vorname / Prénom"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Nachname / Nom"
      },
      "permitNumber": {
        "type": "string",
        "description": "Permit number"
      },
      "permitType": {
        "type": "string",
        "description": "Permit type",
        "enum": [
          "Temporary",
          "Permanent",
          "Long-term",
          "Student",
          "Work",
          "Family",
          null
        ],
        "nullable": true
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Staatsangehörigkeit"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Geburtsdatum",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Geburtsort"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Geschlecht",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Ausstellungsdatum",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Gültig bis",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Ausstellende Behörde"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Anschrift"
      },
      "remarks": {
        "type": "string",
        "description": "Remarks / Annotations"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "permitNumber",
      "permitType",
      "nationality",
      "issueDate",
      "expirationDate"
    ]
  }
}