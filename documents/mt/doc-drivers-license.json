{
  "id": "doc-drivers-license",
  "name": "Driver's License",
  "description": "Driver's License — Malta",
  "references": [
    {
      "title": "Transport Malta",
      "url": "https://www.transport.gov.mt/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Isem"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Kunjom"
      },
      "licenseNumber": {
        "type": "string",
        "description": "License number"
      },
      "categories": {
        "type": "string",
        "description": "License categories (EU Directive 2006/126/EC)",
        "enum": [
          "AM",
          "A1",
          "A2",
          "A",
          "B1",
          "B",
          "BE",
          "C1",
          "C1E",
          "C",
          "CE",
          "D1",
          "D1E",
          "D",
          "DE",
          null
        ],
        "nullable": true
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Data tat-twelid",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Post tat-twelid"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Data tal-ħruġ",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Data ta' skadenza",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Awtorità li ħarġet"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Indirizz"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Nazzjonalità"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Sess",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      }
    },
    "required": [
      "firstName",
      "lastName",
      "licenseNumber",
      "categories",
      "issueDate",
      "expirationDate"
    ]
  }
}