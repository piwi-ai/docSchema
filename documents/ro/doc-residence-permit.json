{
  "id": "doc-residence-permit",
  "name": "Residence Permit",
  "description": "Residence Permit — Romania",
  "references": [
    {
      "title": "IGI — Permis de ședere",
      "url": "https://igi.mai.gov.ro/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Prenume"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Nume"
      },
      "permitNumber": {
        "type": "string",
        "description": "Permit number"
      },
      "permitType": {
        "type": "string",
        "description": "Permit type",
        "enum": [
          "Temporary",
          "Permanent",
          "Long-term",
          "Student",
          "Work",
          "Family",
          null
        ],
        "nullable": true
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Cetățenie"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Data nașterii",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Locul nașterii"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Sex",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Data eliberării",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Data expirării",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Emis de"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Adresa"
      },
      "remarks": {
        "type": "string",
        "description": "Remarks / Annotations"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "permitNumber",
      "permitType",
      "nationality",
      "issueDate",
      "expirationDate"
    ]
  }
}