{
  "id": "doc-passport",
  "name": "Passport",
  "description": "Passport — Sweden",
  "references": [
    {
      "title": "Polisen — Pass",
      "url": "https://polisen.se/tjanster-tillstand/pass-och-nationellt-id-kort/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Förnamn"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Efternamn"
      },
      "passportNumber": {
        "type": "string",
        "description": "Passport number"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Medborgarskap"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Födelsedatum",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Födelseort"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Kön",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Utfärdandedatum",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Giltighetstid",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Utfärdande myndighet"
      },
      "issuingCountry": {
        "type": "string",
        "description": "Issuing country / Code"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "passportNumber",
      "nationality",
      "dateOfBirth",
      "sex",
      "issueDate",
      "expirationDate"
    ]
  }
}