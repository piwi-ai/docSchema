{
  "id": "doc-residence-permit",
  "name": "Residence Permit",
  "description": "Residence Permit — United Kingdom",
  "references": [
    {
      "title": "GOV.UK — Biometric Residence Permit",
      "url": "https://www.gov.uk/biometric-residence-permits",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Given name"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Surname"
      },
      "permitNumber": {
        "type": "string",
        "description": "Permit number"
      },
      "permitType": {
        "type": "string",
        "description": "Permit type",
        "enum": [
          "Temporary",
          "Permanent",
          "Long-term",
          "Student",
          "Work",
          "Family",
          null
        ],
        "nullable": true
      },
      "nationality": {
        "type": "string",
        "description": "Nationality"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth"
      },
      "sex": {
        "type": "string",
        "description": "Sex",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority"
      },
      "address": {
        "type": "string",
        "description": "Residential address"
      },
      "remarks": {
        "type": "string",
        "description": "Remarks / Annotations"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "permitNumber",
      "permitType",
      "nationality",
      "issueDate",
      "expirationDate"
    ]
  }
}