{
  "id": "doc-passport",
  "name": "Passport",
  "description": "Passport — Greece",
  "references": [
    {
      "title": "Hellenic Passport Center",
      "url": "http://www.passport.gov.gr/en/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Όνομα"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Επώνυμο"
      },
      "passportNumber": {
        "type": "string",
        "description": "Passport number"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Υπηκοότητα"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Ημερομηνία γέννησης",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Τόπος γέννησης"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Φύλο",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Ημερομηνία έκδοσης",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Ημερομηνία λήξης",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Αρχή έκδοσης"
      },
      "issuingCountry": {
        "type": "string",
        "description": "Issuing country / Code"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "passportNumber",
      "nationality",
      "dateOfBirth",
      "sex",
      "issueDate",
      "expirationDate"
    ]
  }
}