          },
          "customerName": {
            "type": "string",
            "description": "Name of the customer/recipient"
          },
          "customerAddress": {
            "type": "string",
            "description": "Customer address"
          },
          "subtotal": {
            "type": "number",
            "description": "Subtotal amount before tax (USD, numbers only, no dollar sign)"
