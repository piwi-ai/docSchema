{
  "id": "doc-drivers-license",
  "name": "Driver's License",
  "description": "Driver's License — Estonia",
  "references": [
    {
      "title": "Transport Administration",
      "url": "https://www.transpordiamet.ee/en",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Eesnimi"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Perekonnanimi"
      },
      "licenseNumber": {
        "type": "string",
        "description": "License number"
      },
      "categories": {
        "type": "string",
        "description": "License categories (EU Directive 2006/126/EC)",
        "enum": [
          "AM",
          "A1",
          "A2",
          "A",
          "B1",
          "B",
          "BE",
          "C1",
          "C1E",
          "C",
          "CE",
          "D1",
          "D1E",
          "D",
          "DE",
          null
        ],
        "nullable": true
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Sünniaeg",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Sünnikoht"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Väljaandmise kuupäev",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Kehtiv kuni",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Väljaandja"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Aadress"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Kodakondsus"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Sugu",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      }
    },
    "required": [
      "firstName",
      "lastName",
      "licenseNumber",
      "categories",
      "issueDate",
      "expirationDate"
    ]
  }
}