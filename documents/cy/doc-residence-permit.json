{
  "id": "doc-residence-permit",
  "name": "Residence Permit",
  "description": "Residence Permit — Cyprus",
  "references": [
    {
      "title": "Civil Registry — Residence",
      "url": "http://www.moi.gov.cy/moi/crmd/crmd.nsf/index_en/index_en",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Όνομα"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Επώνυμο"
      },
      "permitNumber": {
        "type": "string",
        "description": "Permit number"
      },
      "permitType": {
        "type": "string",
        "description": "Permit type",
        "enum": [
          "Temporary",
          "Permanent",
          "Long-term",
          "Student",
          "Work",
          "Family",
          null
        ],
        "nullable": true
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Υπηκοότητα"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Ημερομηνία γέννησης",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Τόπος γέννησης"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Φύλο",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Ημερομηνία έκδοσης",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Ημερομηνία λήξης",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Αρχή έκδοσης"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Διεύθυνση"
      },
      "remarks": {
        "type": "string",
        "description": "Remarks / Annotations"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "permitNumber",
      "permitType",
      "nationality",
      "issueDate",
      "expirationDate"
    ]
  }
}