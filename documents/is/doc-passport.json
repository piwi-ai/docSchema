{
  "id": "doc-passport",
  "name": "Passport",
  "description": "Passport — Iceland",
  "references": [
    {
      "title": "Registers Iceland — Passport",
      "url": "https://www.skra.is/english/individuals/passport-and-id-card/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Fornafn"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Eftirnafn"
      },
      "passportNumber": {
        "type": "string",
        "description": "Passport number"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Ríkisfang"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Fæðingardagur",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Fæðingarstaður"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Kyn",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Útgáfudagur",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Gildir til",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Útgefandi"
      },
      "issuingCountry": {
        "type": "string",
        "description": "Issuing country / Code"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "passportNumber",
      "nationality",
      "dateOfBirth",
      "sex",
      "issueDate",
      "expirationDate"
    ]
  }
}