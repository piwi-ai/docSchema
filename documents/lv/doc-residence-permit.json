{
  "id": "doc-residence-permit",
  "name": "Residence Permit",
  "description": "Residence Permit — Latvia",
  "references": [
    {
      "title": "PMLP — Residence Permit",
      "url": "https://www.pmlp.gov.lv/en/residence-permit",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Vārds"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Uzvārds"
      },
      "permitNumber": {
        "type": "string",
        "description": "Permit number"
      },
      "permitType": {
        "type": "string",
        "description": "Permit type",
        "enum": [
          "Temporary",
          "Permanent",
          "Long-term",
          "Student",
          "Work",
          "Family",
          null
        ],
        "nullable": true
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Pilsonība"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Dzimšanas datums",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Dzimšanas vieta"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Dzimums",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Izsniegšanas datums",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Derīgs līdz",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Izsniedzējiestāde"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Adrese"
      },
      "remarks": {
        "type": "string",
        "description": "Remarks / Annotations"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "permitNumber",
      "permitType",
      "nationality",
      "issueDate",
      "expirationDate"
    ]
  }
}