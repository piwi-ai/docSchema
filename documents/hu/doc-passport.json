{
  "id": "doc-passport",
  "name": "Passport",
  "description": "Passport — Hungary",
  "references": [
    {
      "title": "Kormányablak — Útlevél",
      "url": "https://kormanyablak.hu/hu/feladatkorok/10",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Utónév"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Vezetéknév"
      },
      "passportNumber": {
        "type": "string",
        "description": "Passport number"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Állampolgárság"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Születési dátum",
        "pattern": "^\\d{4}\\.\\d{2}\\.\\d{2}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Születési hely"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Nem",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Kiállítás dátuma",
        "pattern": "^\\d{4}\\.\\d{2}\\.\\d{2}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Érvényesség",
        "pattern": "^\\d{4}\\.\\d{2}\\.\\d{2}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Kiállító hatóság"
      },
      "issuingCountry": {
        "type": "string",
        "description": "Issuing country / Code"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "passportNumber",
      "nationality",
      "dateOfBirth",
      "sex",
      "issueDate",
      "expirationDate"
    ]
  }
}