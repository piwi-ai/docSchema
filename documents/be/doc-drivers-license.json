{
  "id": "doc-drivers-license",
  "name": "Driver's License",
  "description": "Driver's License — Belgium",
  "references": [
    {
      "title": "Belgium.be — Driving license",
      "url": "https://mobilit.belgium.be/en/road/driving-licence",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Prénom / Voornaam"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Nom / Achternaam"
      },
      "licenseNumber": {
        "type": "string",
        "description": "License number"
      },
      "categories": {
        "type": "string",
        "description": "License categories (EU Directive 2006/126/EC)",
        "enum": [
          "AM",
          "A1",
          "A2",
          "A",
          "B1",
          "B",
          "BE",
          "C1",
          "C1E",
          "C",
          "CE",
          "D1",
          "D1E",
          "D",
          "DE",
          null
        ],
        "nullable": true
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Date de naissance",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Lieu de naissance"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Date de délivrance",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Date d'expiration",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Autorité de délivrance"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Adresse"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Nationalité"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Sexe",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      }
    },
    "required": [
      "firstName",
      "lastName",
      "licenseNumber",
      "categories",
      "issueDate",
      "expirationDate"
    ]
  }
}