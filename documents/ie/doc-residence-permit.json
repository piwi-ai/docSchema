{
  "id": "doc-residence-permit",
  "name": "Residence Permit",
  "description": "Residence Permit — Ireland",
  "references": [
    {
      "title": "Immigration Service Delivery",
      "url": "https://www.irishimmigration.ie/registering-your-immigration-permission/how-to-register-your-immigration-permission/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Céadainm"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Sloinne"
      },
      "permitNumber": {
        "type": "string",
        "description": "Permit number"
      },
      "permitType": {
        "type": "string",
        "description": "Permit type",
        "enum": [
          "Temporary",
          "Permanent",
          "Long-term",
          "Student",
          "Work",
          "Family",
          null
        ],
        "nullable": true
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Náisiúntacht"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Dáta breithe",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Áit bhreithe"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Gnéas",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Dáta eisiúna",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Dáta éaga",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Údarás eisiúna"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Seoladh"
      },
      "remarks": {
        "type": "string",
        "description": "Remarks / Annotations"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "permitNumber",
      "permitType",
      "nationality",
      "issueDate",
      "expirationDate"
    ]
  }
}