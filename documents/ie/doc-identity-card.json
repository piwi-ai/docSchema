{
  "id": "doc-identity-card",
  "name": "Identity Document",
  "description": "Identity document (ID Card, Passport, Driver License)",
  "references": [
    {
      "title": "DFA — Passport Card",
      "url": "https://www.dfa.ie/passportcard/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Céadainm"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Sloinne"
      },
      "personalId": {
        "type": "string",
        "description": "Personal ID number / PPS Number",
        "pattern": "^\\d{7}[A-Z]{1,2}$"
      },
      "documentType": {
        "type": "string",
        "description": "Document type / Cineál cáipéise"
      },
      "documentNumber": {
        "type": "string",
        "description": "Document number / Uimhir cáipéise"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Dáta eisiúna",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Dáta éaga",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Údarás eisiúna"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Áit bhreithe"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Dáta breithe",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Seoladh"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Náisiúntacht"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Gnéas",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "personalId",
      "documentType",
      "documentNumber",
      "issueDate",
      "expirationDate"
    ]
  }
}