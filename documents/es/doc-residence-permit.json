{
  "id": "doc-residence-permit",
  "name": "Residence Permit",
  "description": "Residence Permit — Spain",
  "references": [
    {
      "title": "Extranjería",
      "url": "https://extranjeros.inclusion.gob.es/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Nombre"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Apellidos"
      },
      "permitNumber": {
        "type": "string",
        "description": "Permit number"
      },
      "permitType": {
        "type": "string",
        "description": "Permit type",
        "enum": [
          "Temporary",
          "Permanent",
          "Long-term",
          "Student",
          "Work",
          "Family",
          null
        ],
        "nullable": true
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Nacionalidad"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Fecha de nacimiento",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Lugar de nacimiento"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Sexo",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Fecha de expedición",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Fecha de caducidad",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Autoridad de expedición"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Domicilio"
      },
      "remarks": {
        "type": "string",
        "description": "Remarks / Annotations"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "permitNumber",
      "permitType",
      "nationality",
      "issueDate",
      "expirationDate"
    ]
  }
}