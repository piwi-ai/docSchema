{
  "id": "doc-drivers-license",
  "name": "Driver's License",
  "description": "Driver's License — Denmark",
  "references": [
    {
      "title": "Borger.dk — Kørekort",
      "url": "https://www.borger.dk/transport-trafik-rejser/Biler-og-koerekort/Koerekort",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Fornavn"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Efternavn"
      },
      "licenseNumber": {
        "type": "string",
        "description": "License number"
      },
      "categories": {
        "type": "string",
        "description": "License categories (EU Directive 2006/126/EC)",
        "enum": [
          "AM",
          "A1",
          "A2",
          "A",
          "B1",
          "B",
          "BE",
          "C1",
          "C1E",
          "C",
          "CE",
          "D1",
          "D1E",
          "D",
          "DE",
          null
        ],
        "nullable": true
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Fødselsdato",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Fødested"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Udstedelsesdato",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Udløbsdato",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Udstedende myndighed"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Adresse"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Nationalitet"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Køn",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      }
    },
    "required": [
      "firstName",
      "lastName",
      "licenseNumber",
      "categories",
      "issueDate",
      "expirationDate"
    ]
  }
}