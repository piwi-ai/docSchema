{
  "id": "doc-identity-card",
  "name": "Identity Document",
  "description": "Identity document (ID Card, Passport, Driver License)",
  "references": [
    {
      "title": "Polisen — Nationellt id-kort",
      "url": "https://polisen.se/tjanster-tillstand/pass-och-nationellt-id-kort/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Förnamn"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Efternamn"
      },
      "personalId": {
        "type": "string",
        "description": "Personal ID number / Personnummer",
        "pattern": "^\\d{8}-?\\d{4}$"
      },
      "documentType": {
        "type": "string",
        "description": "Document type / Dokumenttyp"
      },
      "documentNumber": {
        "type": "string",
        "description": "Document number / Dokumentnummer"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Utfärdandedatum",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Giltighetstid",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Utfärdande myndighet"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Födelseort"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Födelsedatum",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Adress"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Medborgarskap"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Kön",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "personalId",
      "documentType",
      "documentNumber",
      "issueDate",
      "expirationDate"
    ]
  }
}