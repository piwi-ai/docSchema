{
  "id": "doc-drivers-license",
  "name": "Driver's License",
  "description": "Driver's License — Hungary",
  "references": [
    {
      "title": "Kormányablak — Vezetői engedély",
      "url": "https://kormanyablak.hu/hu/feladatkorok/28",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Utónév"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Vezetéknév"
      },
      "licenseNumber": {
        "type": "string",
        "description": "License number"
      },
      "categories": {
        "type": "string",
        "description": "License categories (EU Directive 2006/126/EC)",
        "enum": [
          "AM",
          "A1",
          "A2",
          "A",
          "B1",
          "B",
          "BE",
          "C1",
          "C1E",
          "C",
          "CE",
          "D1",
          "D1E",
          "D",
          "DE",
          null
        ],
        "nullable": true
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Születési dátum",
        "pattern": "^\\d{4}\\.\\d{2}\\.\\d{2}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Születési hely"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Kiállítás dátuma",
        "pattern": "^\\d{4}\\.\\d{2}\\.\\d{2}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Érvényesség",
        "pattern": "^\\d{4}\\.\\d{2}\\.\\d{2}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Kiállító hatóság"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Lakcím"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Állampolgárság"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Nem",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      }
    },
    "required": [
      "firstName",
      "lastName",
      "licenseNumber",
      "categories",
      "issueDate",
      "expirationDate"
    ]
  }
}