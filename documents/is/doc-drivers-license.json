{
  "id": "doc-drivers-license",
  "name": "Driver's License",
  "description": "Driver's License — Iceland",
  "references": [
    {
      "title": "Island.is — Driving licence",
      "url": "https://island.is/en/driving-licence",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Fornafn"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Eftirnafn"
      },
      "licenseNumber": {
        "type": "string",
        "description": "License number"
      },
      "categories": {
        "type": "string",
        "description": "License categories (EU Directive 2006/126/EC)",
        "enum": [
          "AM",
          "A1",
          "A2",
          "A",
          "B1",
          "B",
          "BE",
          "C1",
          "C1E",
          "C",
          "CE",
          "D1",
          "D1E",
          "D",
          "DE",
          null
        ],
        "nullable": true
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Fæðingardagur",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Fæðingarstaður"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Útgáfudagur",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Gildir til",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Útgefandi"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Heimilisfang"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Ríkisfang"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Kyn",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      }
    },
    "required": [
      "firstName",
      "lastName",
      "licenseNumber",
      "categories",
      "issueDate",
      "expirationDate"
    ]
  }
}