{
  "id": "doc-drivers-license",
  "name": "Driver's License",
  "description": "Driver's License — Romania",
  "references": [
    {
      "title": "DGPCI — Permise",
      "url": "https://dgpci.mai.gov.ro/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Prenume"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Nume"
      },
      "licenseNumber": {
        "type": "string",
        "description": "License number"
      },
      "categories": {
        "type": "string",
        "description": "License categories (EU Directive 2006/126/EC)",
        "enum": [
          "AM",
          "A1",
          "A2",
          "A",
          "B1",
          "B",
          "BE",
          "C1",
          "C1E",
          "C",
          "CE",
          "D1",
          "D1E",
          "D",
          "DE",
          null
        ],
        "nullable": true
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Data nașterii",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Locul nașterii"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Data eliberării",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Data expirării",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Emis de"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Adresa"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Cetățenie"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Sex",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      }
    },
    "required": [
      "firstName",
      "lastName",
      "licenseNumber",
      "categories",
      "issueDate",
      "expirationDate"
    ]
  }
}