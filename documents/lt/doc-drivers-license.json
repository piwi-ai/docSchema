{
  "id": "doc-drivers-license",
  "name": "Driver's License",
  "description": "Driver's License — Lithuania",
  "references": [
    {
      "title": "Regitra",
      "url": "https://www.regitra.lt/en/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Vardas"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Pavardė"
      },
      "licenseNumber": {
        "type": "string",
        "description": "License number"
      },
      "categories": {
        "type": "string",
        "description": "License categories (EU Directive 2006/126/EC)",
        "enum": [
          "AM",
          "A1",
          "A2",
          "A",
          "B1",
          "B",
          "BE",
          "C1",
          "C1E",
          "C",
          "CE",
          "D1",
          "D1E",
          "D",
          "DE",
          null
        ],
        "nullable": true
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Gimimo data",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Gimimo vieta"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Išdavimo data",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Galioja iki",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Išdavusi institucija"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Adresas"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Pilietybė"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Lytis",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      }
    },
    "required": [
      "firstName",
      "lastName",
      "licenseNumber",
      "categories",
      "issueDate",
      "expirationDate"
    ]
  }
}