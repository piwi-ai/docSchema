{
  "id": "doc-drivers-license",
  "name": "Driver's License",
  "description": "Driver's License — United Kingdom",
  "references": [
    {
      "title": "GOV.UK — Driving licences",
      "url": "https://www.gov.uk/browse/driving/driving-licences",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Given name"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Surname"
      },
      "licenseNumber": {
        "type": "string",
        "description": "License number"
      },
      "categories": {
        "type": "string",
        "description": "License categories (EU Directive 2006/126/EC)",
        "enum": [
          "AM",
          "A1",
          "A2",
          "A",
          "B1",
          "B",
          "BE",
          "C1",
          "C1E",
          "C",
          "CE",
          "D1",
          "D1E",
          "D",
          "DE",
          null
        ],
        "nullable": true
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority"
      },
      "address": {
        "type": "string",
        "description": "Residential address"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality"
      },
      "sex": {
        "type": "string",
        "description": "Sex",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      }
    },
    "required": [
      "firstName",
      "lastName",
      "licenseNumber",
      "categories",
      "issueDate",
      "expirationDate"
    ]
  }
}