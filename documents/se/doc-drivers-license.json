{
  "id": "doc-drivers-license",
  "name": "Driver's License",
  "description": "Driver's License — Sweden",
  "references": [
    {
      "title": "Transportstyrelsen — Körkort",
      "url": "https://www.transportstyrelsen.se/sv/vagtrafik/Korkort/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Förnamn"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Efternamn"
      },
      "licenseNumber": {
        "type": "string",
        "description": "License number"
      },
      "categories": {
        "type": "string",
        "description": "License categories (EU Directive 2006/126/EC)",
        "enum": [
          "AM",
          "A1",
          "A2",
          "A",
          "B1",
          "B",
          "BE",
          "C1",
          "C1E",
          "C",
          "CE",
          "D1",
          "D1E",
          "D",
          "DE",
          null
        ],
        "nullable": true
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Födelsedatum",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Födelseort"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Utfärdandedatum",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Giltighetstid",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Utfärdande myndighet"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Adress"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Medborgarskap"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Kön",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      }
    },
    "required": [
      "firstName",
      "lastName",
      "licenseNumber",
      "categories",
      "issueDate",
      "expirationDate"
    ]
  }
}