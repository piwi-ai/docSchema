{
  "id": "doc-passport",
  "name": "Passport",
  "description": "Passport — Switzerland",
  "references": [
    {
      "title": "ch.ch — Passport",
      "url": "https://www.ch.ch/en/documents-and-register-extracts/passport/",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Vorname / Prénom"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Nachname / Nom"
      },
      "passportNumber": {
        "type": "string",
        "description": "Passport number"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Staatsangehörigkeit"
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Geburtsdatum",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Geburtsort"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Geschlecht",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Ausstellungsdatum",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Gültig bis",
        "pattern": "^\\d{2}\\.\\d{2}\\.\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Ausstellende Behörde"
      },
      "issuingCountry": {
        "type": "string",
        "description": "Issuing country / Code"
      },
      "mrz": {
        "type": "string",
        "description": "Machine Readable Zone (MRZ)"
      }
    },
    "required": [
      "firstName",
      "lastName",
      "passportNumber",
      "nationality",
      "dateOfBirth",
      "sex",
      "issueDate",
      "expirationDate"
    ]
  }
}