{
  "id": "doc-drivers-license",
  "name": "Driver's License",
  "description": "Driver's License — Cyprus",
  "references": [
    {
      "title": "Road Transport Department",
      "url": "http://www.mcw.gov.cy/mcw/rtd/rtd.nsf/index_en/index_en",
      "type": "documentation"
    }
  ],
  "jsonSchema": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name / Όνομα"
      },
      "lastName": {
        "type": "string",
        "description": "Last name / Επώνυμο"
      },
      "licenseNumber": {
        "type": "string",
        "description": "License number"
      },
      "categories": {
        "type": "string",
        "description": "License categories (EU Directive 2006/126/EC)",
        "enum": [
          "AM",
          "A1",
          "A2",
          "A",
          "B1",
          "B",
          "BE",
          "C1",
          "C1E",
          "C",
          "CE",
          "D1",
          "D1E",
          "D",
          "DE",
          null
        ],
        "nullable": true
      },
      "dateOfBirth": {
        "type": "string",
        "description": "Date of birth / Ημερομηνία γέννησης",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "placeOfBirth": {
        "type": "string",
        "description": "Place of birth / Τόπος γέννησης"
      },
      "issueDate": {
        "type": "string",
        "description": "Issue date / Ημερομηνία έκδοσης",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "expirationDate": {
        "type": "string",
        "description": "Expiration date / Ημερομηνία λήξης",
        "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
      },
      "issuingAuthority": {
        "type": "string",
        "description": "Issuing authority / Αρχή έκδοσης"
      },
      "address": {
        "type": "string",
        "description": "Residential address / Διεύθυνση"
      },
      "nationality": {
        "type": "string",
        "description": "Nationality / Υπηκοότητα"
      },
      "sex": {
        "type": "string",
        "description": "Sex / Φύλο",
        "enum": [
          "M",
          "F",
          "X",
          null
        ],
        "nullable": true
      }
    },
    "required": [
      "firstName",
      "lastName",
      "licenseNumber",
      "categories",
      "issueDate",
      "expirationDate"
    ]
  }
}